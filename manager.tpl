<div class="col-xs-12 col-sm-6 col-lg-4 team-member-col">
	<div class="team-member">
		<div class="team-photo">
			<img src="/uploads/users/{photo}" alt="">
			<div class="big-triangle"></div>
			<div class="big-triangle2"></div>
			<a class="big-icon big-icon-plus" href="/manager?id={id}"><i class="jfont">î „</i></a>
			<div class="team-description">
				<div>
					<div class="team-desc-line">
						<span class="team-icon-circle">
							<i class="fa fa-phone"></i>
						</span>
						<span>{phone}</span>
					</div>
					<div class="team-desc-line">
						<span class="team-icon-circle">
							<i class="fa fa-envelope fa-sm"></i>
						</span>
						<span>{email}</span>
					</div>
					<div class="team-social-cont">
						<div class="team-social">
							<a class="team-icon-circle" href="{vk}">
								<i class="fa fa-vk"></i>
							</a>
						</div>
						<div class="team-social">
							<a class="team-icon-circle" href="{odnoklassniki}">
								<i class="fa fa-odnoklassniki"></i>
							</a>
						</div>
						<div class="team-social">
							<a class="team-icon-circle" href="{facebook}">
								<i class="fa fa-facebook"></i>
							</a>
						</div>
						<div class="team-social">
							<a class="team-icon-circle" href="skype:{skype}">
								<i class="fa fa-skype"></i>
							</a>
						</div>
					</div>
					<p class="team-text">
					{description}
					</p>
				</div>
			</div>
		</div>
		<div class="team-name">
			<p class="managers_name">{first_name} {last_name}<span class="special-color">.</span></p>
			<p class="managers_group">{group}</p>

		</div>
	</div>
</div>